ExistBall = (NOT (P BallOutOfRange))
ExistGoal = (NOT (P GoalOutOfRange))
ExistPositionTarget = (NOT (P PositionTargetOutOfRange))
ExistOrientationTarget = (NOT (P OrientationTargetOutOfRange))
#	ExistObst = (NOT (P ObstOutOfRange))

OwnBall = (OR (AND (P BallN) (P BallClose)) (AND (P BallS) (P BallClose)))
AlignedWithBall = (OR (AND (P BallN) (P GoalN)) (AND (P BallS) (P GoalS)))
OnPositionTarget = (P PositionTargetVeryClose)
OnOrientationTarget = (OR (P OrientationTargetNrN) (P OrientationTargetNrS))