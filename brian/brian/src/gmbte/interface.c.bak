                    NULL);
  g_signal_connect ((gpointer) insertCrispCancel, "clicked",
                    G_CALLBACK (on_insertCrispCancel_clicked),
                    NULL);

  /* Store pointers to all widgets, for use by lookup_widget(). */
  GLADE_HOOKUP_OBJECT_NO_REF (insertCrispWindow, insertCrispWindow, "insertCrispWindow");
  GLADE_HOOKUP_OBJECT (insertCrispWindow, vbox3, "vbox3");
  GLADE_HOOKUP_OBJECT (insertCrispWindow, insertCrispTitle, "insertCrispTitle");
  GLADE_HOOKUP_OBJECT (insertCrispWindow, vbox4, "vbox4");
  GLADE_HOOKUP_OBJECT (insertCrispWindow, table2, "table2");
