  GLADE_HOOKUP_OBJECT (createDialog, label1, "label1");
  GLADE_HOOKUP_OBJECT_NO_REF (createDialog, dialog_action_area1, "dialog_action_area1");
  GLADE_HOOKUP_OBJECT (createDialog, okbuttonDialog, "okbuttonDialog");

  return createDialog;
}

