GtkWidget*
create_insertCrispWindow (void)
{
  GtkWidget *insertCrispWindow;
  GtkWidget *vbox3;
  GtkWidget *insertCrispTitle;
  GtkWidget *vbox4;
  GtkWidget *table2;
  GtkWidget *insertCrispName;
  GtkWidget *insertCrispReliability;
  GtkWidget *entry10;
  GtkWidget *entry11;
  GtkWidget *hbox2;
