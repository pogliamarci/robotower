  insertCrispWindow = gtk_window_new (GTK_WINDOW_TOPLEVEL);
  gtk_window_set_title (GTK_WINDOW (insertCrispWindow), _("New Crisp Data"));

  vbox3 = gtk_vbox_new (FALSE, 0);
  gtk_widget_show (vbox3);
  gtk_container_add (GTK_CONTAINER (insertCrispWindow), vbox3);

  insertCrispTitle = gtk_label_new (_("Insert Crisp Data Values"));
  gtk_widget_show (insertCrispTitle);
  gtk_box_pack_start (GTK_BOX (vbox3), insertCrispTitle, FALSE, FALSE, 0);

  vbox4 = gtk_vbox_new (FALSE, 0);
  gtk_widget_show (vbox4);
  gtk_box_pack_start (GTK_BOX (vbox3), vbox4, TRUE, TRUE, 0);

  table2 = gtk_table_new (2, 2, FALSE);
  gtk_widget_show (table2);
  gtk_box_pack_start (GTK_BOX (vbox4), table2, TRUE, TRUE, 0);

  insertCrispName = gtk_label_new (_("Data Name"));
  gtk_widget_show (insertCrispName);
  gtk_table_attach (GTK_TABLE (table2), insertCrispName, 0, 1, 0, 1,
                    (GtkAttachOptions) (GTK_FILL),
                    (GtkAttachOptions) (0), 0, 0);
  gtk_misc_set_alignment (GTK_MISC (insertCrispName), 0, 0.5);

  insertCrispReliability = gtk_label_new (_("Data Reliability"));
  gtk_widget_show (insertCrispReliability);
  gtk_table_attach (GTK_TABLE (table2), insertCrispReliability, 0, 1, 1, 2,
                    (GtkAttachOptions) (GTK_FILL),
                    (GtkAttachOptions) (0), 0, 0);
  gtk_misc_set_alignment (GTK_MISC (insertCrispReliability), 0, 0.5);

  entry10 = gtk_entry_new ();
  gtk_widget_show (entry10);
  gtk_table_attach (GTK_TABLE (table2), entry10, 1, 2, 0, 1,
                    (GtkAttachOptions) (GTK_EXPAND | GTK_FILL),
                    (GtkAttachOptions) (0), 0, 0);

  entry11 = gtk_entry_new ();
  gtk_widget_show (entry11);
  gtk_table_attach (GTK_TABLE (table2), entry11, 1, 2, 1, 2,
                    (GtkAttachOptions) (GTK_EXPAND | GTK_FILL),
                    (GtkAttachOptions) (0), 0, 0);

  hbox2 = gtk_hbox_new (FALSE, 0);
  gtk_widget_show (hbox2);
  gtk_box_pack_start (GTK_BOX (vbox4), hbox2, TRUE, TRUE, 0);

  insertCrispScroll = gtk_scrolled_window_new (NULL, NULL);
  gtk_widget_show (insertCrispScroll);
  gtk_box_pack_start (GTK_BOX (hbox2), insertCrispScroll, TRUE, TRUE, 0);

  insertCrispOutput = gtk_text_view_new ();
  gtk_widget_show (insertCrispOutput);
  gtk_container_add (GTK_CONTAINER (insertCrispScroll), insertCrispOutput);
  gtk_text_buffer_set_text (gtk_text_view_get_buffer (GTK_TEXT_VIEW (insertCrispOutput)),
	_("Insert new Crisp Data..."), -1);

  insertCrispButtonBox = gtk_vbutton_box_new ();
  gtk_widget_show (insertCrispButtonBox);
  gtk_box_pack_start (GTK_BOX (hbox2), insertCrispButtonBox, FALSE, FALSE, 0);
  gtk_container_set_border_width (GTK_CONTAINER (insertCrispButtonBox), 4);
  gtk_button_box_set_layout (GTK_BUTTON_BOX (insertCrispButtonBox), GTK_BUTTONBOX_SPREAD);

