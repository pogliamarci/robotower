GtkWidget*
create_fileselection (void)
{
  GtkWidget *fileselection;
  GtkWidget *ok_fileSelection;
  GtkWidget *cancel_fileSelection;

  fileselection = gtk_file_selection_new (_("Seleziona file"));
  gtk_container_set_border_width (GTK_CONTAINER (fileselection), 10);
  GTK_WINDOW (fileselection)->type = GTK_WINDOW_POPUP;
  gtk_window_set_position (GTK_WINDOW (fileselection), GTK_WIN_POS_CENTER);

  ok_fileSelection = GTK_FILE_SELECTION (fileselection)->ok_button;
  gtk_widget_show (ok_fileSelection);
  GTK_WIDGET_SET_FLAGS (ok_fileSelection, GTK_CAN_DEFAULT);

  cancel_fileSelection = GTK_FILE_SELECTION (fileselection)->cancel_button;
  gtk_widget_show (cancel_fileSelection);
  GTK_WIDGET_SET_FLAGS (cancel_fileSelection, GTK_CAN_DEFAULT);

  g_signal_connect ((gpointer) ok_fileSelection, "clicked",
                    G_CALLBACK (on_ok_fileSelection_clicked),
                    NULL);
  g_signal_connect ((gpointer) cancel_fileSelection, "clicked",
                    G_CALLBACK (on_cancel_fileSelection_clicked),
                    NULL);

  /* Store pointers to all widgets, for use by lookup_widget(). */
  GLADE_HOOKUP_OBJECT_NO_REF (fileselection, fileselection, "fileselection");
  GLADE_HOOKUP_OBJECT_NO_REF (fileselection, ok_fileSelection, "ok_fileSelection");
  GLADE_HOOKUP_OBJECT_NO_REF (fileselection, cancel_fileSelection, "cancel_fileSelection");

  return fileselection;
}

GtkWidget*
