  GLADE_HOOKUP_OBJECT (insertCrispWindow, entry10, "entry10");
  GLADE_HOOKUP_OBJECT (insertCrispWindow, entry11, "entry11");
  GLADE_HOOKUP_OBJECT (insertCrispWindow, hbox2, "hbox2");
  GLADE_HOOKUP_OBJECT (insertCrispWindow, insertCrispScroll, "insertCrispScroll");
  GLADE_HOOKUP_OBJECT (insertCrispWindow, insertCrispOutput, "insertCrispOutput");
